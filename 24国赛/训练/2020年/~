#include<iostream>
using namespace std;
struct fen
{
    int a;
    int b;
};
int minx(int a,int b)
{
    int max=a>b?a:b;
    for(int i=max;i<a*b;i++)
    {
        if(i%a==0&&i%b==0)
        {
            
            return i;//删除break 
        }
    }

}
struct fen sum(fen x,fen y)
{
    fen q;
    int max_=minx(x.b,y.b);
    q.b=max_;
    q.a=x.a*max_/x.b+y.a*max_/y.b;
    return q;//缺少返回值 
}
struct fen hua(fen q)
{
    int i,min=q.a<=q.b?q.a:q.b;
    for( i=min;i>0;i--)
    {
    	
        if(q.a%i==0&&q.b%i==0)
            break;
    }
    if(i==0){
    	q.a=0;
    	q.b=q.b;
    }//考虑q分子为0的情况 
    else{
    	q.a=q.a/i;
    	q.b=q.b/i;
    }
    return q;
}

int main()
{
    int n;
    char x;
    cin>>n;
    fen a[n];
    for(int i=0;i<n;i++){
     	cin>>a[i].a>>x>>a[i].b;
     }
    fen sum_=a[0];
for(int i=1;i<n;i++)
{
        sum_=sum(sum_,a[i]);
        //cout<<sum_.a<<'/'<<sum_.b<<endl; 
        sum_=hua(sum_);
}
    fen q;
    q.b=sum_.b*n;
    q.a=sum_.a;
     q=hua(q);
     if(q.b==1)
        cout<<q.a;
    else
        cout<<q.a<<"/"<<q.b;
    return 0;
}

